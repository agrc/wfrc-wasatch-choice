{
  "name": "wfrc",
  "version": "5.4.9",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "del ./dist/** && vite build",
    "build:stage": "del ./dist/** && vite build --mode stage --outDir dist/vision-map",
    "cypress:open": "concurrently \"pnpm start\" \"cypress open\"",
    "cypress:run": "start-server-and-test start http://localhost:5173 'cypress run'",
    "format": "prettier . -w",
    "lint": "eslint src",
    "preview": "vite preview",
    "start": "vite",
    "storybook": "storybook dev -p 9009",
    "test": "vitest",
    "update-config-docs": "del ./docs/** && jsonschema2md -d public -o docs -h false -x - -n",
    "zip": "bestzip build.zip dist/*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson"
    ],
    "singleQuote": true
  },
  "dependencies": {
    "@arcgis/core": "^4.34.8",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "bootstrap": "^5.3.8",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "downshift": "^9.0.13",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonschema": "^1.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "perfect-scrollbar": "^1.5.6",
    "react": "^19.2.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.3",
    "react-dual-listbox": "^6.0.3",
    "react-error-boundary": "^6.0.0",
    "react-fast-compare": "^3.2.2",
    "react-i18next": "^15.7.4",
    "react-loader-spinner": "^8.0.0",
    "react-perfect-scrollbar": "^1.5.8",
    "reactstrap": "^9.2.3",
    "typeface-montserrat": "^1.1.13"
  },
  "devDependencies": {
    "@adobe/jsonschema2md": "^8.0.9",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/pluginutils": "^5.3.0",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-links": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@testing-library/cypress": "^10.1.0",
    "@ugrc/eslint-config": "^1.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "bestzip": "^2.2.1",
    "concurrently": "^9.2.1",
    "cypress": "^15.8.1",
    "cypress-wait-until": "^3.0.2",
    "del-cli": "^7.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "10.1.10",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.20",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^19.2.3",
    "sass": "^1.97.0",
    "start-server-and-test": "^2.1.3",
    "storybook": "^10.1.10",
    "typescript": "^5.9.3",
    "url": "^0.11.4",
    "vite": "^7.3.0",
    "vitest": "^3.2.4"
  }
}
