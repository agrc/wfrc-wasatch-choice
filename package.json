{
  "name": "wfrc",
  "version": "4.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "array-move": "^4.0.0",
    "bootstrap": "^4.5.0",
    "downshift": "^5.4.5",
    "esri-loader": "^2.14.0",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "jsonschema": "^1.4.1",
    "lodash.debounce": "^4.0.8",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "perfect-scrollbar": "^1.5.5",
    "query-string": "^6.13.1",
    "react": "^17.0.2",
    "react-device-detect": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-dual-listbox": "^2.2.0",
    "react-fast-compare": "^3.2.0",
    "react-i18next": "^11.16.9",
    "react-loader-spinner": "^5.1.4",
    "react-perfect-scrollbar": "^1.5.8",
    "react-scripts": "^5.0.1",
    "react-sortable-hoc": "^2.0.0",
    "reactstrap": "^8.10.1",
    "typeface-montserrat": "^1.1.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run update-config-docs && react-scripts build",
    "build-stage": "npm run update-config-docs && ./node_modules/.bin/env-cmd -f ./.env.stage npm run build",
    "test": "react-scripts test",
    "test:once": "npm run lint && npm test -- --watchAll=false && npm run cypress:run",
    "lint": "eslint src",
    "cypress:run": "start-server-and-test 'npm start' http://localhost:3000 'cypress run'",
    "cypress:open": "concurrently \"npm start\" \"cypress open\"",
    "eject": "react-scripts eject",
    "release": "standard-version",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "update-config-docs": "del ./docs/** && jsonschema2md -d public -o docs -h false -x - -n"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "cy": true,
      "Cypress": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@adobe/jsonschema2md": "^4.1.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addon-storyshots": "^6.5.6",
    "@storybook/addons": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.6",
    "@testing-library/cypress": "^7.0.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react": "^12.1.5",
    "babel-preset-react-app": "^10.0.1",
    "concurrently": "^5.2.0",
    "cypress": "^6.5.0",
    "cypress-pipe": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "env-cmd": "^10.1.0",
    "grunt": "^1.5.3",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-compress": "^1.6.0",
    "grunt-ssh": "^0.12.9",
    "identity-obj-proxy": "^3.0.0",
    "load-grunt-tasks": "^5.1.0",
    "node-sass": "^7.0.1",
    "react-test-renderer": "^16.13.1",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^1.14.0",
    "webpack": "^5.73.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(array-move)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "typeface-montserrat": "identity-obj-proxy"
    }
  }
}
