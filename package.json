    "typeface-montserrat": "^1.1.13"
  },
  "scripts": {
    "build:stage": "npm run update-config-docs && vite build --mode stage",
    "build": "npm run update-config-docs && vite build",
    "cypress:open": "concurrently \"npm start\" \"cypress open\"",
    "cypress:run": "start-server-and-test 'npm start' http://localhost:3000 'cypress run'",
    "format": "prettier . -w",
    "lint": "eslint src",
    "preview": "vite preview",
    "release": "standard-version",
    "start": "vite",
    "storybook": "start-storybook -p 9009",
    "test": "vitest",
    "update-config-docs": "del ./docs/** && jsonschema2md -d public -o docs -h false -x - -n"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "prettier",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended"
    ],
    "ignorePatterns": [
      "dist/**"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "globals": {
      "cy": true,
      "Cypress": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "devDependencies": {
    "@adobe/jsonschema2md": "^4.1.6",
    "@rollup/pluginutils": "^4.2.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^5.2.0",
    "core-js": "^3.23.2",
    "cypress": "^6.5.0",
    "cypress-pipe": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^3.0.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "grunt": "^1.5.3",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-compress": "^1.6.0",
    "grunt-ssh": "^0.12.9",
    "identity-obj-proxy": "^3.0.0",
    "load-grunt-tasks": "^5.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.52.3",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
