{
  "name": "wfrc",
  "version": "5.4.8",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "del ./dist/** && vite build",
    "build:stage": "del ./dist/** && vite build --mode stage --outDir dist/vision-map",
    "cypress:open": "concurrently \"pnpm start\" \"cypress open\"",
    "cypress:run": "start-server-and-test start http://localhost:5173 'cypress run'",
    "format": "prettier . -w",
    "lint": "eslint src",
    "preview": "vite preview",
    "start": "vite",
    "storybook": "storybook dev -p 9009",
    "test": "vitest",
    "update-config-docs": "del ./docs/** && jsonschema2md -d public -o docs -h false -x - -n",
    "zip": "bestzip build.zip dist/*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson"
    ],
    "singleQuote": true
  },
  "dependencies": {
    "@arcgis/core": "^4.31.6",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "downshift": "^9.0.8",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "jsonschema": "^1.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "perfect-scrollbar": "^1.5.6",
    "react": "^19.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-dual-listbox": "^6.0.3",
    "react-error-boundary": "^5.0.0",
    "react-fast-compare": "^3.2.2",
    "react-i18next": "^15.4.0",
    "react-loader-spinner": "^6.1.6",
    "react-perfect-scrollbar": "^1.5.8",
    "reactstrap": "^9.2.3",
    "typeface-montserrat": "^1.1.13"
  },
  "devDependencies": {
    "@adobe/jsonschema2md": "^8.0.2",
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@rollup/pluginutils": "^5.1.4",
    "@storybook/addon-actions": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@testing-library/cypress": "^10.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "bestzip": "^2.2.1",
    "concurrently": "^9.1.2",
    "cypress": "^13.17.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "happy-dom": "^16.8.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.8",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^19.0.0",
    "sass": "^1.83.4",
    "start-server-and-test": "^2.0.10",
    "storybook": "^8.5.3",
    "typescript": "^5.7.3",
    "url": "^0.11.4",
    "vite": "^6.0.11",
    "vitest": "^2.1.8"
  }
}
